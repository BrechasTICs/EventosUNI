<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">

<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
	xmlns:g='urn:import:com.google.gwt.user.client.ui' ui:generateFormat='com.google.gwt.i18n.rebind.format.PropertiesFormat'
	ui:generateKeys='com.google.gwt.i18n.rebind.keygen.MD5KeyGenerator'
	ui:generateLocales='default'>

	<g:HTMLPanel width="100%" height="15" styleName="sesion">
		<g:Grid width="100%" height="15" >
			<g:row>
				<g:customCell>
					<g:Label text="EventosUNI" ui:field="sesion" styleName="sesion-label" height="15" width="70%" horizontalAlignment="ALIGN_LEFT"/>
				</g:customCell>
				<g:customCell>
					<g:Label text="Anónimo" ui:field="nickname" styleName="nickname-label" height="15" horizontalAlignment="ALIGN_RIGHT"/>
				</g:customCell>
				<g:customCell>
					<g:Button text="Cerrar Sesión" ui:field="loginButton" height="15" styleName="logout-button" />
				</g:customCell>
			</g:row>
		</g:Grid>

	</g:HTMLPanel>
</ui:UiBinder>
