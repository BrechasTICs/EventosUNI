<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">

<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
	xmlns:g='urn:import:com.google.gwt.user.client.ui' ui:generateFormat='com.google.gwt.i18n.rebind.format.PropertiesFormat'
	ui:generateKeys='com.google.gwt.i18n.rebind.keygen.MD5KeyGenerator' xmlns:w="urn:import:com.google.gwt.widget.client"
	ui:generateLocales='default'>

	<g:HTMLPanel>
		<g:FormPanel>
			<g:Grid>
				<g:row>
					<g:customCell>
						<g:Button text="Eliminar Contacto" ui:field="eliminarContacto"></g:Button>
					</g:customCell>
				</g:row>
				<g:row>
					<g:customCell>
						<g:Label text="Tipo contacto" />
					</g:customCell>
					<g:customCell>
						<g:ListBox ui:field="tipo">
							<g:item>
								Teléfono
							</g:item>							
							<g:item>
								Dirección
							</g:item>
							<g:item>
								E-mail
							</g:item>
							<g:item>
								Página Web
							</g:item>
						</g:ListBox>
					</g:customCell>
				</g:row>
				<g:row>
					<g:customCell>
						<g:Label text="Valor contacto" />
					</g:customCell>
					<g:customCell>
						<g:TextArea width="238" ui:field="valor"/>
					</g:customCell>
				</g:row>
			</g:Grid>
		</g:FormPanel>
	</g:HTMLPanel>
</ui:UiBinder>
