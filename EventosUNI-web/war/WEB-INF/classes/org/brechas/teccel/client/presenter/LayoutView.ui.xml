<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">

<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
	xmlns:g='urn:import:com.google.gwt.user.client.ui' ui:generateFormat='com.google.gwt.i18n.rebind.format.PropertiesFormat'
	ui:generateKeys='com.google.gwt.i18n.rebind.keygen.MD5KeyGenerator'
	ui:generateLocales='default'>

	<g:HTMLPanel width="100%" height="100%" styleName="global">
		<g:DockPanel width="100%" height="100%"
			horizontalAlignment="ALIGN_CENTER"
			borderWidth="0">
			<g:Dock direction="NORTH" height="5%" horizontalAlignment="ALIGN_RIGHT">
				<g:HTMLPanel ui:field="loginContentPanel" width="100%">
				</g:HTMLPanel>
			</g:Dock>
			<g:Dock direction="SOUTH" height="5%" horizontalAlignment="ALIGN_CENTER">
				<g:HTMLPanel ui:field="footContentPanel" width="100%">
				</g:HTMLPanel>
			</g:Dock>
			<g:Dock direction="CENTER" horizontalAlignment="ALIGN_CENTER"
				verticalAlignment="ALIGN_MIDDLE" >
				<g:DockPanel spacing="10" width="75%" height="100%"
					horizontalAlignment="ALIGN_CENTER">
					<g:Dock direction="NORTH" horizontalAlignment="ALIGN_CENTER"
						verticalAlignment="ALIGN_TOP">
						<g:HTMLPanel ui:field="headContentPanel"
							styleName="header" height="25%">
							<g:Image url="images/banner.png" altText="EVENTOSUNI" />
						</g:HTMLPanel>
					</g:Dock>
					<g:Dock direction="CENTER" width="100%" height="100%"
						horizontalAlignment="ALIGN_CENTER">
						<g:HTMLPanel ui:field="mainContentPanel" />
					</g:Dock>
					<g:Dock direction="WEST" width="10%" height="100%"
						horizontalAlignment="ALIGN_CENTER" verticalAlignment="ALIGN_TOP">
						<g:VerticalPanel height="100%" verticalAlignment="ALIGN_TOP">
							<g:HTMLPanel ui:field="menuContentPanel" height="50%"
								width="100%" />
							<g:HTMLPanel height="20%">
								<g:Image url="images/logopeque.png" altText="EVENTOSUNI" />
							</g:HTMLPanel>
						</g:VerticalPanel>
					</g:Dock>
				</g:DockPanel>
			</g:Dock>
		</g:DockPanel>
	</g:HTMLPanel>
</ui:UiBinder>
